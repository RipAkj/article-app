
<%= form_with(scope: :session, url: login_path,local: true) do |f| %>
    <p>
    <%= f.label :email %><br>
    <%= f.email_field :email %>
    </p>
    <p>
    <%= f.label :password %><br>
    <%= f.password_field :password_digest %>
    </p>
    <p>
    <%= f.submit "Log In" %>
    </p>
<% end %>
<%= link_to 'Go back to users page', root_path %>
